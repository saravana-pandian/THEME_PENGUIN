</template>
<template id="place_in_three" inherit_id="website.snippets" name="Place in three">
    <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
        <t t-snippet="theme_Penguin.s_content_quality_list"
